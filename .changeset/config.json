{
  "$schema": "https://unpkg.com/@changesets/config@3.1.1/schema.json",
  "changelog": ["@changesets/changelog-github", { "repo": "pizzajsdev/dragswag" }],
  "baseBranch": "origin/main",
  "commit": false,
  "access": "public",
  "updateInternalDependencies": "patch",
  "ignore": ["dragswag-examples"],
