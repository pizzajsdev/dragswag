  "peerDependencies": {
    "react": ">=16.8.0",
    "snapdrag-beta": "*"
  },
  "devDependencies": {
    "@types/react": ">=16.8.0",
    "snapdrag-beta": "*"
  },
  "scripts": {
    "build": "rm -rf ./dist && microbundle --jsx React.createElement"
  },
