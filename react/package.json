{
  "name": "@snapdrag/react",
  "type": "module",
  "version": "1.0.0",
  "source": "src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "umd:main": "./dist/index.umd.js",
  "unpkg": "./dist/index.umd.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
