{
  "name": "dragswag-docs",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rm -rf ./build && VITE_APP_BASE_PATH=/dragswag/ react-router build",
    "postbuild": "mv ./build/client/dragswag/* ./build/client/ && mv ./build/client ./build/dragswag",
    "dev": "VITE_APP_BASE_PATH=/dragswag/ react-router dev --open --host",
    "lint": "npm run typecheck",
    "start": "pnpm build && pnpm dlx http-server ./build -o /dragswag",
    "start:ssr": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@pizzajsdev/app-router-fs": "^0.3.2",
    "@react-router/node": "^7.6.0",
    "@react-router/serve": "^7.6.0",
    "clsx": "^2.1.1",
    "dragswag": "workspace:*",
    "isbot": "^5.1.28",
    "lodash": "^4.17.21",
    "lucide-react": "^0.511.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.0",
    "styled-components": "^6.1.18",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@react-router/dev": "^7.6.0",
    "@tailwindcss/vite": "^4.1.7",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.19",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
